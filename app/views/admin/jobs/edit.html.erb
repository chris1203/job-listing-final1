<div class="col-md-4 col-md-offset-4">
  <h2 class="text-center">Edit a Job</h2>
  <hr>
  <%= simple_form_for [:admin, @job] do |f| %>
    <%= f.input :title %>
    <%= f.input :description %>
    <%= f.input :wage_lower_bound, label: "Lower Salary" %>
    <%= f.input :wage_upper_bound, label: "Upper Salary" %>
    <%= f.input :contact_email %>
    <%= f.submit "Submit", class: "btn btn-primary" %>
  <% end %>
</div>
